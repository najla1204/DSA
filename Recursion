//print 1 to n using backtrack
/*import java.util.*;
public class Recursion {
    static void print(int i,int n)
    {
        if(i<1)
        {
            return;
        }
        print(i-1,n);          //first the function gets called until i<1
        System.out.println(i);    //after all the function call gets over it prints from the back that is 1 to n
    }
    public static void main(String[] args) {
        int n;
        Scanner in = new Scanner(System.in);
        n = in.nextInt();
        print(n,n);
    }
}*/
