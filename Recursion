//Recursion function to print Nth Fibonacci numbers
//time complexity order of 2 to the power of n.
import java.util.*;

public class Recursion {
    static int fibo(int n) {
        if (n <= 1) {
            return n;
        }
        return fibo(n - 1) + fibo(n - 2);  //1 1 2 3 5 8 13
    }

    public static void main(String[] args) {
        int n;
        Scanner in = new Scanner(System.in);
        n = in.nextInt();
        System.out.println(fibo(n));
    }
}





