//Functional way of finding the sum of n numbers.
/*import java.util.*;
public class Recursion {
    static int sum(int n)
    {
        if(n==0)
        {
            return 0;
        }
        return n+sum(n-1);
    }
    public static void main(String[] args) {
        int n;
        Scanner in = new Scanner(System.in);
        n = in.nextInt();
        System.out.println(sum(n));
    }
}*/
//functional method of finding factorial using recursion
//time complexity : O(n)
//space complexity : O(n)
/*import java.util.*;
public class Recursion {
    static int fact(int n)
    {
        if(n==0)
        {
            return 1;
        }
        return n * fact(n-1);
    }
    public static void main(String[] args) {
        int n;
        Scanner in = new Scanner(System.in);
        n = in.nextInt();
        System.out.println(fact(n));
    }
}*/
//reverse an array using recursion.
/*import java.util.*;

import static java.util.Collections.reverse;

public class Recursion {
    static void reverse(int[] arr,int n,int i)
    {
        if(i>=n/2)
        {
            return;
        }
        int temp = arr[i];
        arr[i] = arr[n-i-1];
        arr[n-i-1] = temp;
        reverse(arr,n,i+1);
    }
    public static void main(String[] args) {
        int n;
        Scanner in = new Scanner(System.in);
        n = in.nextInt();
        int[] arr = new int[n];
        for(int i =0;i<n;i++)
        {
          arr[i] = in.nextInt();
        }
        reverse(arr,n , 0 );
        System.out.println(Arrays.toString(arr));
    }
}*/
